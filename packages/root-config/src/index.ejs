        "@react-mf/things": "https://react.microfrontends.app/things/7f209a1ed9ac9690835c57a3a8eb59c17114bb1d/react-mf-things.js",
        "@react-mf/styleguide/": "https://react.microfrontends.app/styleguide/64b2c715165299efaa799ec377824bb2766bd70f/",
        "@react-mf/root-config/": "https://react.microfrontends.app/root-config/5029372b482f81f4ad50804ab051875148c3bcdc/",
